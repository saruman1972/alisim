<?xml version="1.0" encoding="UTF-8"?>
<case name="Card Verify" desc="Card Verify">
  <request tag="CSVReq" desc="Card Verify Request">
    <field name="version" />
    <field name="instId" />
    <field name="certId" />
    <field name="date" />
    <field name="accountName" show="true" />
    <field name="bankCardNo" show="true" />
    <field name="bankCardType" show="true" />
    <field name="certType" show="true" />
    <field name="certNo" show="true" />
    <field name="mobilePhone" show="true" />
    <field name="isCheckCertificateNo" show="true" />
    <field name="isCheckMobilePhone" show="true" />
    <field name="isCheckAccountName" show="true" />
    <field name="expiryDate" show="true" />
    <field name="cvv2" show="true" />
    <field name="channelSystemId" show="true" />
  </request>
  <response tag="CSVRes" desc="Card Verify Response">
    <field name="version" present="mandate" />
    <field name="instId" present="mandate" />
    <field name="certId" present="mandate" />
    <field name="date" present="mandate" />
    <field name="bankCardNo" present="mandate" />
    <field name="channelSystemId" present="optional" />
  </response>
</case>

