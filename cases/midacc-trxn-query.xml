<?xml version="1.0" encoding="UTF-8"?>
<case name="Fund Transfer Query" desc="Fund Transfer Query">
  <request tag="FTQReq" desc="Fund Transfer Query Request">
    <field name="version" />
    <field name="instId" />
    <field name="certId" />
    <field name="date" />
    <field name="beginDate" show="true" />
    <field name="endDate" show="true" />
    <field name="accountNo" show="true" />
    <field name="queryPage" show="true" />
    <field name="channelSystemId" show="true" />
    <field name="limitType" show="true" />
  </request>
  <response tag="IADRRes" desc="Fund Transfer Query Response">
    <field name="version" present="mandate" />
    <field name="instId" present="mandate" />
    <field name="certId" present="mandate" />
    <field name="date" present="mandate" />
    <field name="originalDate" present="mandate" />
    <field name="beginDate" present="mandate" />
    <field name="endDate" present="mandate" />
    <field name="accountNo" present="mandate" />
    <field name="totalCount" present="mandate" />
    <field name="totalPage" present="mandate" />
    <field name="currPage" present="mandate" />
    <field name="TransInfoList" present="mandate" />
    <field name="channelSystemId" present="optional" />
    <field name="limitType" present="optional" />
  </response>
</case>
