<?xml version="1.0" encoding="UTF-8"?>
<case name="Intermediate Account DailyReport" desc="Intermediate Account DailyReport">
  <request tag="IADRReq" desc="Intermediate Account DailyReport Request">
    <field name="version" />
    <field name="instId" />
    <field name="certId" />
    <field name="date" />
    <field name="bankDate" show="true" />
    <field name="currency" show="true" />
    <field name="channelSystemId" show="true" />
    <field name="limitType" show="true" />
  </request>
  <response tag="IADRRes" desc="Intermediate Account DailyReport Response">
    <field name="version" present="mandate" />
    <field name="instId" present="mandate" />
    <field name="certId" present="mandate" />
    <field name="bankDate" present="mandate" />
    <field name="currency" present="mandate" />
    <field name="balance" present="mandate" />
    <field name="dbalance" present="mandate" />
    <field name="ddalance" present="mandate" />
    <field name="dcalance" present="mandate" />
    <field name="channelSystemId" present="optional" />
    <field name="limitType" present="optional" />
  </response>
</case>
